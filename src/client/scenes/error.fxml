<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view_controllers.ErrorController">
   <children>
      <ImageView fx:id="image" fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="30.0" />
      <Label fx:id="title" text="Label" AnchorPane.leftAnchor="150.0" AnchorPane.topAnchor="60.0">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label fx:id="message" text="Label" AnchorPane.leftAnchor="150.0" AnchorPane.topAnchor="110.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <HBox fx:id="optionButtons" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
   </children>
</AnchorPane>
